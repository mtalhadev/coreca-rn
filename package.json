{
  "scripts": {
    "prebuild:dev": "CONFIG_ENV=development expo prebuild",
    "build": "yarn prebuild:dev && CONFIG_ENV=development eas build --profile development",
    "start": "FIREBASE_OFFICIAL_EMULATE=false expo start --dev-client",
    "start:emulate": "FIREBASE_OFFICIAL_EMULATE=true expo start --dev-client",
    "start:emulate-only-functions": "FUNCTION_EMULATE=true expo start --dev-client",
    "check-build:prod": "eas build:list",
    "build:local": "yarn prebuild:dev && CONFIG_ENV=development eas build --profile development --local",
    "doctor": "expo-cli doctor --fix-dependencies",
    "submit:preview": "eas submit --profile preview",
    "submit": "eas submit --profile production",
    "build:preview": "yarn prebuild:dev && eas build --profile preview",
    "build:preview:local": "yarn prebuild:dev && CONFIG_ENV=development eas build --profile preview --local",
    "prebuild:prod": "CONFIG_ENV=production expo prebuild",
    "build:prod": "yarn prebuild:prod && eas build --profile production",
    "build:prod:local": "yarn prebuild:prod && CONFIG_ENV=production eas build --profile production --local",
    "update:preview": "eas update --branch preview",
    "update:prod": "eas update --branch production",
    "add-device:ios": "eas device:create",
    "start:go": "expo start",
    "reset-expo": "npx install-expo-modules@^46.0.10",
    "reset-project": "npx react-native-clean-project",
    "test": "jest --coverage",
    "test:chat": "jest -- __tests__/chat",
    "test:chat-components": "jest -- __tests__/chat/component",
    "test:ci": "npm run test -- __tests__/chat __tests__/usecases -u --watchAll=false --ci --coverage",
    "prepare": "husky install",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,tsx}'",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "fix": "yarn format:fix && yarn lint:fix",
    "update:license": "license-checker --production  --json --direct --out ./assets/licenses.json --customPath licenseFormat.json",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build-env:ios": "bundle install --path vender/bundler && sudo rbenv rehash",
    "postinstall": "patch-package"
  },
