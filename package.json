  "engines": {
    "node": "16"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "./jestSetupFile.js"
    ],
    "setupFilesAfterEnv": [
      "./jestSetupAfterEnv.js",
      "./__mocks__/mockFirebase.ts",
      "./__mocks__/mockImageManipulator.ts"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "^.+\\.svg$": "./__mocks__/jestSvgTransformer.js"
